import java.util.*;
public class Lee{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		int [] a=new int[t];
		for(int i=0;i<t;i++)
			a[i]=sc.nextInt();
		for(int i=0;i<t;i++)
		{
			if(a[i]%4==0)
				System.out.println("YES");
			else
				System.out.println("NO");
		}
	}
}
