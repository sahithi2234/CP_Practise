import java.util.*;
public class AkelaOne{

     public static void main(String []args){
          Scanner sc=new Scanner(System.in);
         int n=sc.nextInt();
         int[] a=new int[n];
         int ans=0;
         for(int i=0;i<n;i++)
         {
             a[i]=sc.nextInt();
             if(i==0)
                ans=a[i];
            else
                ans=ans^a[i];
         }
         System.out.println(ans);
         
     }
}
